    {"evento": "pedido_vista", "texto_tramitacao": "concedida vista"},
    {"evento": "designado_relator", 
      "texto_tramitacao": "(designa.*relator(a|) da matéria)|((à|ao|a).* senador(a|).*para ((emitir|emissão de) relatório|relatar a (presente |)matéria(.*|)$))"},
    {"evento": "desarquivada", "id_situacao": "^149$"},
    {"evento": "aprovacao_substitutivo", "texto_tramitacao": "aprovado o substitutivo"},
    {"evento": "aprovacao_projeto", "texto_tramitacao": "aprovado o projeto"},
    {"evento": "inicio_prazo_emendas", "id_situacao": "^20$"},
    {"evento": "fim_prazo_emendas", "id_situacao": "^153$"},
    {"evento": "remetida_a_sancao", "id_situacao": "^57$"},
    {"evento": "vetada", "id_situacao": "^57$"},
    {"evento": "pronta_para_pauta", "id_situacao": "^42$"},
    {"evento": "aprovacao_audiencia_publica", "texto_tramitacao": "(aprovou|aprovado).* requerimento .* audiência(s)* pública(s)*"}
