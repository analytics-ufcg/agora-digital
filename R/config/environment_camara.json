  "eventos": [
    {"evento": "aprovacao_parecer", "regex": "^aprovado (?!.*requerimento).*parecer.*$", "tipo":"votacao"},
    {"evento": "requerimento_redistribuicao", "regex": "^apresentação do requerimento de redistribuição", "tipo":"despacho"},
    {"evento": "requerimento_apensacao", "regex": "^apresentação do requerimento de apensação", "tipo":"despacho"},
    {"evento": "requerimento_urgencia", "regex": "^apresentação do requerimento de urgência", "tipo":"despacho"},
