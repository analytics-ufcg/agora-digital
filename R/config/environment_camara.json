    {"evento": "projeto_reconstituido", "regex": "reconstituído", "tipo":"despacho"},
    {"evento": "desarquivamento", "regex": "^desarquivad(o|a) nos termos do artigo", "tipo":"despacho"},
    {"evento": "alteracao_de_regime", "regex": "^alteração do regime de tramitação desta proposição", "tipo":"despacho"},
    {"evento": "redistribuicao", "regex": "^novo despacho|atualização do despacho|despacho à.*novo despacho", "tipo":"despacho"},
    {"evento": "distribuicao", "regex": "às* comiss..s*|despacho à", "tipo":"despacho"},
    {"evento": "comissao_especial", "regex": "recebimento pela p", "tipo":"despacho"},
    {"evento": "designado_relator", "regex": "^designad(o|a) relator(a)*", "tipo":"despacho"},
    {"evento": "voto_em_separado", "regex": "^apresentação do voto em separado", "tipo":"votacao"},
    {"evento": "apresentacao_pl", "regex": "^apresentação do projeto de lei|^apresentação da proposta de emenda à constituição", "tipo":"despacho"},
    {"evento": "retirada_de_pauta", "regex": "^retirado de pauta", "tipo":"votacao"},
    {"evento": "pedido_de_vista", "regex": "^(vista conjunta aos|vista ao)", "tipo":"votacao"},
    {"evento": "inicio_prazo_emendas", "regex": "^prazo para emendas|^abertura de prazo para emendas|^reabertura de prazo para emendas", "tipo":"discussao"},
    {"evento": "fim_prazo_emendas", "regex": "^encerrado o prazo para emendas", "tipo":"discussao"},
    {"evento": "arquivamento", "regex": "^arquivad(o|a) nos termos", "tipo":"despacho"},
    {"evento": "parecer_pela_adequacao_financeira_e_orcamentaria",  "id_situacao": 322, "regex": "pela adequa..o financeira e or.ament.ria", "tipo":"votacao"},
    {"evento": "parecer_pela_aprovacao_com_substitutivo", "id_situacao": 322, "regex": "^(?!.*pela adequa..o financeira e or.ament.ria).*substitutivo.*$", "tipo":"votacao"},
    {"evento": "parecer_pela_aprovacao", "id_situacao": 322, "regex": "^(?!.*substitutivo)(?!.*pela adequa..o financeira e or.ament.ria).*aprova.*$", "tipo":"votacao"},
    {"evento": "parecer_pela_rejeicao", "id_situacao": 322, "regex": "^(?!.*aprova)(?!.*substitutivo)(?!.*emenda).*rejei..o.*$", "tipo":"votacao"},
    {"evento": "parecer", "id_situacao": 322, "regex": "^(?!.*aprova)(?!.*rejei)(?!.*substitutivo).*parecer.*$", "tipo":"votacao"},
    {"evento": "virada_de_casa", "regex": "(^(remessa|remetido) ao senado federal.*)|(recebido o ofício.* do sf, que comunico que o senado federal aprovou, em revisão.*)", "tipo": "votacao"},
    {"evento": "remetida_a_sancao", "regex": "(projeto( foi|) encaminhado à sanção presidencial)|(remessa à sanção.*)", "tipo":"votacao"},
    {"evento": "transformada_lei", "regex": "transformado na lei ordinária", "tipo": "votacao"},
    {"evento": "vetada_totalmente", "regex": "proposição vetada totalmente", "tipo": "votacao"},
    {"evento": "parecer_comissao", "regex": "^encaminhada à publicação. parecer da comissão.*", "tipo": "votacao"},
    {"evento": "req_apresentacao", "regex": "", "tipo": "discussao"},
